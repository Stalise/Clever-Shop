{
    "**/*.(js|jsx|ts|tsx|css|scss|html|json|yaml|md)": ["prettier --write"],
    "**/*.(js|jsx|ts|tsx)": ["eslint --fix", "eslint --max-warnings=0"],
    "**/*.(ts|tsx)": "tsc-files --incremental false src/types/declaration.d.ts",
    "**/*.(js|ts)": ["stylelint --fix", "stylelint --max-warnings=0"]
}
