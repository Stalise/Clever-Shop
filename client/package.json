{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "test:unit": "jest --config jest.config.ts --passWithNoTests --coverage",
        "test:unit:report": "cross-env JEST_REPORT=true jest --config jest.config.ts --passWithNoTests --coverage; open coverage/index.html",
        "format": "prettier --write \"./\"",
        "lint": "eslint -c .eslintrc.json --ext .js,.jsx,.ts,.tsx --max-warnings=0 \"./\"",
        "lint:fix": "yarn lint --fix",
        "styles": "stylelint \"src/**/*.ts\" --max-warnings=0",
        "styles:fix": "stylelint \"src/**/*.ts\" --fix",
        "check:ts": "tsc --incremental false",
        "check:all": "yarn lint && yarn styles && yarn check:ts",
        "convert-svg": "node_modules/@svgr/cli/bin/svgr src/assets/static --template svgr-template.js && yarn lint:fix"
    },
    "dependencies": {
        "next": "13.4.10",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "styled-components": "6.0.4"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "13.4.10",
        "@stylelint/postcss-css-in-js": "0.38.0",
        "@svgr/cli": "8.0.1",
        "@types/jest": "29.5.3",
        "@types/node": "20.4.2",
        "@types/react": "18.2.15",
        "@types/react-dom": "18.2.7",
        "@typescript-eslint/eslint-plugin": "6.1.0",
        "@typescript-eslint/parser": "6.1.0",
        "cross-env": "7.0.3",
        "eslint": "8.45.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-check-file": "2.5.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jest": "27.2.3",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "jest": "29.6.1",
        "jest-environment-jsdom": "29.6.1",
        "postcss": "8.4.26",
        "postcss-syntax": "0.36.2",
        "prettier": "3.0.0",
        "stylelint": "15.10.1",
        "stylelint-config-rational-order": "0.1.2",
        "stylelint-config-standard": "34.0.0",
        "stylelint-config-styled-components": "0.1.1",
        "stylelint-processor-styled-components": "1.10.0",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "tsc-files": "1.1.4",
        "typescript": "5.1.6"
    }
}
